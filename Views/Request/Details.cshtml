@model WebApplication_aspnet.Models.Request

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles de Oficinas</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-3">Nombre</dt>
        <dd class = "col-sm-9">@Html.DisplayFor(model => model.Title)</dd>

        <dt class = "col-sm-3">Descripción</dt>
        <dd class = "col-sm-9">@Html.DisplayFor(model => model.Description)</dd>

        <dt class = "col-sm-3">Fecha Creación</dt>
        <dd class = "col-sm-9">@Html.DisplayFor(model => model.Dcreated_at)</dd>
       
        <dt class = "col-sm-3">Status</dt>
        <dd class = "col-sm-9">@Html.DisplayFor(model => model.Status)</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id_request" class="btn btn-warning">Editar</a> 
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>